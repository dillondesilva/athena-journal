{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "athena-be/athena-backend",
          "sidecar": true
        }
      ]
    },
    "fs:default"
  ]
}